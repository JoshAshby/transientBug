+++
theme_color: orange
nav: admin_email
breadcrumbs_top: True
base: skeletons/sidebar
+++

<div class="row">
  <div class="col-md-6">
    <h3>{{email.subject}}</h3>
  </div>
  <div class="col-md-6">
    <span class="pull-right">
      Sent: {{email.formated_created}}
    </span>
  </div>
</div>

<div class="row">
  <div class="col-md-12">
    From <b>{{email.service}}</b> to {{email.who|join(", ")}}<br>
    {% if email.cc_addressess %}
      CC'd: {{email.who_cc|join(", ")}}<br>
    {% endif %}
    {% if email.bcc_addresses %}
      BCC'd: {{email.who_bcc|join(", ")}}<br>
    {% endif %}
  </div>
</div>

<br>
<br>

<div class="row">
  <div class="col-md-12">
<pre>{{email.content["html"]}}</pre>
  </div>
</div>
