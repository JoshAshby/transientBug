            transientBug
          </a>
        </div>

        <!-- Collect the nav links, forms, and other content for toggling -->
        <div class="collapse navbar-collapse navbar-ex1-collapse">
          <ul class="nav navbar-nav">
            <li {{ 'class="active"' if "phots" in nav }}><a href="/phots"><i class="fa fa-picture-o"></i> Phots</a></li>
            <li {{ 'class="active"' if "recipes" in nav }}><a href="/recipes"><i class="fa fa-cutlery"></i> Recipes</a></li>
            <li {{ 'class="active"' if "notes" in nav }}><a href="/notes"><i class="fa fa-file-text"></i> Notes</a></li>
          </ul>

          {% if session.id %}
            <ul class="nav navbar-nav navbar-right" style="margin-right: 15px">
              {% if session.has_group("screenshots") %}
                <li {{ 'class="active"' if "screenshots" in nav }}><a href="/screenshots">Scrns</a></li>
              {% endif %}
              <li class="dropdown {{ 'active' if "admin" in nav }}">
              <a href="#" class="dropdown-toggle" data-toggle="dropdown">Heya, {{session.user.username}} <i class="fa fa-caret-down"></i></a>
                <ul class="dropdown-menu">
                  {% if session.has_group("admin") %}
                    <li><a href="/admin"><i class="fa fa-cogs"></i> Admin</a></li>
                  {% endif %}
                  <li><a href="/account"><i class="fa fa-cog"></i> Settings</a></li>
                  <li class="divider"></li>
                  <li><a href="/logout"><i class="fa fa-sign-out"></i> Logout</a></li>
                </ul>
              </li>
            </ul>
          {% else %}
            <ul class="nav navbar-nav navbar-right" style="margin-right: 15px">
              <li><a id="nav_extra_menu_btn" href="#" data-html="true" data-content='<form role="logic" action="/login" method="POST"><div class="form-group"><input type="text" class="form-control" placeholder="Username..." name="username"></div><div class="form-group"><input type="password" class="form-control" placeholder="Password..." name="password"></div><input type="hidden" name="return-to" value="{{req.url.path}}" /><button type="submit" class="btn btn-default btn-primary btn-block">Login</button></form>' data-container="body" data-title="Login" data-placement="bottom">Heya, stranger <i class="fa fa-chevron-down"></i></a>
              </li>
            </ul>
          {% endif %}
        </div><!-- /.navbar-collapse -->
      </nav>
    {% if has_announcements %}
      <div class="container-alert">
        <div class="row">
          <div class="col-md-1 col-sm-1" style="text-align: center;">
            <i class="fa fa-exclamation fa-4x"></i>
          </div>
          <div class="col-md-10 col-sm-10">
            {% for announcement in announcements %}
              {{announcement}}
            {% endfor %}
          </div>
        </div>
      </div>
    {% endif %}
      <br>
      <div class="container">
        {% if session.alerts %}
          <!-- Begin alerts section -->
          <div class="row">
            <div class="col-md-10 col-md-offset-1">
              {{session._render_alerts()}}
            </div>
          </div>
          <!-- End alerts section -->
        {% endif %}

        <div class="row">
          <div class="col-md-10 col-md-offset-1">
            {{header}}
          </div>
        </div> <!-- /row -->
      </div>
    </div> <!-- /container full -->

    <div class="container">
      <br>
      <div class="row">
        <div class="col-md-10 col-md-offset-1">
          {{body}}
        </div>
      </div>
      <br>
      <hr>
      <footer id="footer">
        <div class="row">
          <div class="col-md-6">
            <a href="https://github.com/JoshAshby/transientbug">transientBug.com 2014</a>
            <a rel="license" href="https://creativecommons.org/licenses/by-nc-sa/3.0/"><img alt="Creative Commons License" style="border-width:0" src="https://i.creativecommons.org/l/by-nc-sa/3.0/80x15.png" /></a><br />This work is licensed under a <a rel="license" href="http://creativecommons.org/licenses/by-nc-sa/3.0/">Creative Commons Attribution-NonCommercial-ShareAlike 3.0 Unported License</a>.
          </div>
          <div class="col-md-6">
            <p class="pull-right"><a href="/about">About</a> | <a href="#top">Back to top</a></p>
          </div>
        </div>
      </footer>
    </div>

    <script src="/static/js/lib/lazyload.js"></script>
    <script src="/static/js/lib/jquery.js"></script>
    <script src="//netdna.bootstrapcdn.com/bootstrap/3.0.0/js/bootstrap.min.js"></script>
    <script src="/static/js/main.js"></script>

    <!-- Begin page scripts -->
    {% for script in scriptFiles %}
      <script src="/static/js/{{script}}.js"></script>
    {% endfor %}

    <script>
      {% if not session.id %}
        $(function() {
          $("#nav_extra_menu_btn").popover();
        });
      {% endif %}

      {{scripts}}
    </script>
    <!-- End page scripts -->
    <br>
  </body>
</html>
