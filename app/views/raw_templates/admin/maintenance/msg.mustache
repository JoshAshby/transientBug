<!doctype html>
<html>
  <head>
  <title>transientBug - Maintenance</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta content="text/html; charset=UTF-8" http-equiv="Content-Type">


    <link href="//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap.no-icons.min.css" rel="stylesheet">
    <link href="//netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.min.css" rel="stylesheet">
    <link href="//netdna.bootstrapcdn.com/bootswatch/3.0.0/cyborg/bootstrap.min.css" rel="stylesheet">

    <!-- Begin page stylsheets -->
    <style>
      @import url(//fonts.googleapis.com/css?family=Maven+Pro:700);

      .navbar-brand, footer {
        font-family: 'Maven Pro', sans-serif;
      }

      .thumbnail {
        border: #2b2b2b solid 2px
      }

      a {
        color: #77B300;
      }

      a:hover {
        color: #507800;
      }

      .dropdown-menu>li>a:hover, .dropdown-menu>li>a:focus {
        background-color: #77B300;
      }

      html,body {
        height:100%;
      }

      h1 {
        font-family: 'Maven Pro', sans-serif;
        font-size:80px;
      }

      /* Custom container */
      .container-full {
        margin: 0 auto;
        width: 100%;
        min-height:100%;
        background-color:#507800;
        color:#eee;
        overflow:hidden;
      }

      .container-full a {
        text-decoration:none;
      }

      .v-center {
        margin-top:7%;
      }

      .popover-title, .popover-content {
        color: black;
      }
    </style>
    <!-- End page style sheets -->

    <script src="//code.jquery.com/jquery.js"></script>
  </head>
  <body>

    <div class="container-full">
      <div class="row">
        <div class="col-lg-12 text-center v-center">
          <h1>{{title}}</h1>
          <p class="lead">{{sub_title}}</p>
          <br><br><br>
          <div class="row">
            <div class="col-md-8 col-md-offset-2">
              <p>{{{contents}}}<br>
              {{#contents}}
                <small> - {{author}}</small>
              {{/contents}}</p>
            </div>
          </div>
        </div>
      </div> <!-- /row -->
      <br><br><br><br><br><br><br><br><br><br><br><br>
    </div> <!-- /container full -->

    <div class="container">
      <div class="row">
        <div class="col-md-10 col-md-offset-1">
        </div>
      </div>

      <hr>
      <footer id="footer">
        <div class="row">
          <div class="col-md-6">
            transientBug.com by transientBug 2013
          </div>
          <div class="col-md-6">
            <p class="pull-right"><a href="#top">Back to top</a></p>
          </div>
        </div>
      </footer>
    </div>

    <script src="//netdna.bootstrapcdn.com/bootstrap/3.0.0/js/bootstrap.min.js"></script>

    <!-- Begin page scripts -->
    <!-- End page scripts -->
  </body>
</html>
